{
  "name": "@tetherto/wdk-wallet-qssn",
  "version": "1.0.0-alpha.1",
  "description": "Quantum-safe wallet development kit for EVM blockchains with ERC-4337 account abstraction and ML-DSA (FIPS 204) post-quantum signatures",
  "keywords": [
    "wdk",
    "wallet",
    "quantum-safe",
    "post-quantum",
    "ml-dsa",
    "fips-204",
    "bip-32",
    "ethereum",
    "erc-4337",
    "account-abstraction"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-wallet-qssn.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@noble/hashes": "1.6.1",
    "@noble/post-quantum": "0.5.2",
    "@scure/bip32": "1.6.0",
    "@scure/bip39": "1.5.0",
    "ethers": "6.14.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.7.4",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "axios": "1.12.2",
    "cookie": "1.0.2",
    "tmp": "0.2.5",
    "ws": "8.18.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
