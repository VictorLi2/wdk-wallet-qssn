{
  "name": "@tetherto/wdk-wallet-qssn",
  "version": "1.0.0-alpha.1",
  "description": "Quantum-safe wallet development kit for EVM blockchains with ERC-4337 account abstraction and ML-DSA (FIPS 204) post-quantum signatures",
  "keywords": [
    "wdk",
    "wallet",
    "quantum-safe",
    "post-quantum",
    "ml-dsa",
    "fips-204",
    "bip-32",
    "ethereum",
    "erc-4337",
    "account-abstraction"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-wallet-qssn.git"
  },
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "dependencies": {
    "@noble/hashes": "1.6.1",
    "@noble/post-quantum": "0.5.2",
    "@scure/bip32": "1.6.0",
    "@scure/bip39": "1.5.0",
    "@tetherto/wdk-wallet": "^1.0.0-beta.1",
    "@tetherto/wdk-wallet-evm": "^1.0.0-beta.3",
    "@wdk-safe-global/relay-kit": "4.1.3",
    "bare-node-runtime": "^1.1.4",
    "ethers": "6.14.4"
  },
  "devDependencies": {
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "overrides": {
    "axios": "1.12.2",
    "cookie": "1.0.2",
    "tmp": "0.2.5",
    "ws": "8.18.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
